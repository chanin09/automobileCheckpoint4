<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="apprtc.thekop.automobilecheckpoint4.MyActivity15m"
    android:background="@drawable/road3">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/c4"
        android:id="@+id/textView17"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="@dimen/h1"
        android:textStyle="bold"
        android:textColor="@color/c1" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b133"
        android:id="@+id/btnsaoha"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/textView17"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b134"
        android:id="@+id/btnchag"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:layout_below="@+id/btnsaoha"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b135"
        android:id="@+id/btnopas"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:layout_below="@+id/btnchag"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b136"
        android:id="@+id/btnpt"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:layout_below="@+id/btnopas"
        android:layout_alignLeft="@+id/btntanut"
        android:layout_alignStart="@+id/btntanut"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b137"
        android:id="@+id/btntana"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:layout_below="@+id/btntanut"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b138"
        android:id="@+id/btntanut"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:layout_below="@+id/btnpt"
        android:layout_alignLeft="@+id/btnchag"
        android:layout_alignStart="@+id/btnchag"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b139"
        android:id="@+id/btntong"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:layout_below="@+id/btntana"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp" />
</RelativeLayout>
