<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="apprtc.thekop.automobilecheckpoint4.MyActivity39m"
    android:background="@drawable/road3">

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b209"
        android:id="@+id/btnnpa"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:textSize="@dimen/b3"
        android:allowUndo="false"
        android:layout_below="@+id/textView40"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/c4"
        android:id="@+id/textView40"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="@dimen/h1"
        android:textStyle="bold"
        android:textColor="@color/c1" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b210"
        android:id="@+id/btnyang"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:textSize="@dimen/b3"
        android:allowUndo="false"
        android:layout_alignTop="@+id/btnnpa"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b211"
        android:id="@+id/btntor"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:textSize="@dimen/b3"
        android:allowUndo="false"
        android:layout_below="@+id/btnnpa"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="70dp" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b212"
        android:id="@+id/btntik"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:textSize="@dimen/b3"
        android:allowUndo="false"
        android:layout_alignTop="@+id/btntor"
        android:layout_alignLeft="@+id/btnyang"
        android:layout_alignStart="@+id/btnyang" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b213"
        android:id="@+id/btnchok"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:textSize="@dimen/b3"
        android:allowUndo="false"
        android:layout_below="@+id/btntor"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="70dp" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b214"
        android:id="@+id/btnkor"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:textSize="@dimen/b3"
        android:allowUndo="false"
        android:layout_alignTop="@+id/btnchok"
        android:layout_alignLeft="@+id/btntik"
        android:layout_alignStart="@+id/btntik" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b215"
        android:id="@+id/btnpasit"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:textSize="@dimen/b3"
        android:allowUndo="false"
        android:layout_below="@+id/btnchok"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="70dp" />

    <Button
        android:layout_width="@dimen/button2"
        android:layout_height="@dimen/button1"
        android:text="@string/b216"
        android:id="@+id/btnpt"
        android:textStyle="bold"
        android:background="#b4ffffff"
        android:textSize="@dimen/b3"
        android:allowUndo="false"
        android:layout_alignTop="@+id/btnpasit"
        android:layout_alignLeft="@+id/btnkor"
        android:layout_alignStart="@+id/btnkor" />
</RelativeLayout>
